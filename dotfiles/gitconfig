[alias]
  d  = diff
  sd = diff --staged
  ud = commit --amend --no-edit
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  wdiff = diff --word-diff=color
  nofm = merge --no-ff
